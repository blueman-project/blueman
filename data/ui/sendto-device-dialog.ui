            <property name="expand">False</property>
            <property name="fill">True</property>
          </packing>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-2">button_cancel</action-widget>
      <action-widget response="-3">button_ok</action-widget>
    </action-widgets>
