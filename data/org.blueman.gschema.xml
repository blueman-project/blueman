<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.blueman" path="/org/blueman/">
      <child name="general" schema="org.blueman.general"/>
      <child name="network" schema="org.blueman.network"/>
      <child name="gsmsettings" schema="org.blueman.gsmsettings"/>
      <child name="netusage" schema="org.blueman.netusage"/>
      <child name="transfer" schema="org.blueman.transfer"/>
  </schema>
  <schema id="org.blueman.general" path="/org/blueman/general/">
      <key type="i" name="window-height">
         <default>200</default>
         <summary>Height Of Window</summary>
         <description>Height of Main Window</description>
      </key>
      <key type="i" name="window-width">
         <default>250</default>
         <summary>Width Of Window</summary>
         <description>Width of Main Window</description>
      </key>
      <key type="i" name="window-posx">
         <default>0</default>
         <summary>X Position Of Window</summary>
         <description>X Position of Main Window</description>
      </key>
      <key type="i" name="window-posy">
         <default>0</default>
         <summary>X Position Of Window</summary>
         <description>X Position of Main Window</description>
      </key>
      <key type="s" name="last-adapter">
         <default>""</default>
         <summary>Last Adapter Used</summary>
         <description>Last Bluetooth Adapter Used</description>
      </key>
	  <key type="s" name="services-last-item">
         <default>""</default>
         <summary>Last Item Used</summary>
         <description>Last Bluetooth Item Used</description>
      </key>
      <key type="b" name="show-toolbar">
         <default>true</default>
         <summary>Show ToolBar</summary>
         <description>Main Window ToolBar</description>
      </key>
	  <key type="b" name="show-statusbar">
         <default>true</default>
         <summary>Show StatusBar</summary>
         <description>Main Window StatusBar</description>
      </key>
      <key type="b" name="latest-last">
         <default>true</default>
         <summary></summary>
         <description></description>
      </key>
      <key type="as" name="plugin-list">
        <default>[]</default>
        <summary></summary>
        <description></description>
      </key>
      <key type="as" name="gsm-list">
        <default>[]</default>
        <summary>List of gsm which have apn settings</summary>
        <description></description>
      </key>
      <key type="as" name="bt-device-list">
        <default>[]</default>
        <summary>List of devices we have netusage data stored.</summary>
        <description></description>
      </key>
      <key type="i" name="discover-timeout">
        <default>60</default>
        <summary>Adapter discovery timeout</summary>
      </key>
  </schema>
</schemalist>
